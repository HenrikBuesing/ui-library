{
  "name": "website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "docusaurus start",
    "build:uil": "NODE_ENV=production node esbuild.mjs",
    "postbuild:uil": "tsc --emitDeclarationOnly --project tsconfig.build.json",
    "build:uil-watch": "NODE_ENV=dev node esbuild.mjs",
    "build": "npm run build:uil && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "clear": "docusaurus clear && npx rimraf src/uil-bundle",
    "serve": "docusaurus serve"
  },
  "dependencies": {
    "@docusaurus/core": "^3.6.2",
    "@docusaurus/preset-classic": "^3.6.2",
    "@docusaurus/theme-live-codeblock": "^3.6.2",
    "@mdx-js/react": "^3.1.0",
    "clsx": "^2.0.0",
    "docusaurus-plugin-sass": "^0.2.5",
    "prism-react-renderer": "^2.4.0",
    "react": "^18.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.6.2",
    "@docusaurus/tsconfig": "^3.6.2",
    "@docusaurus/types": "^3.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  }
